function preview(o){var t=$(o).attr("src"),r=t.replace("s56","l400"),e=t.replace("s56","shuiyinhui");$("#show img").attr("src",r),$("#zoom img").attr("src",e)}!function(o){var t=[];o.each(skus,function(o,r){t.push(r.skuCode)}),GY.iop({url:GY["interface"].checkStatus.url,data:{method:GY["interface"].checkStatus.method,code:o(".product").attr("coding"),type:o(".product").attr("type"),skuCodes:JSON.stringify(t)}}).done(function(t){if((0==t.status||-1==t.status)&&(o("#intro").renderProductIntro(t),o("#show").simpleZoom({zoomBox:"#zoom",markSize:[200,200],zoomSize:[400,400],zoomImg:[800,800]}),o(".product-intro").size()>0)){var r=o(".product").attr("coding"),e=o(".product").attr("type");if(""==r||""==e)return!1;var i=[];o.cookie("gwHistory_n")?-1!=o.cookie("gwHistory_n").indexOf("_")?i=o.cookie("gwHistory_n").split("_"):i.push(o.cookie("gwHistory_n")):i=[];var s=r+"|"+e;i.unshift(s);for(var n=i,c=[],a=0;a<n.length;a++){var u=n[a];-1==o.inArray(u,c)&&c.push(u)}c.length>10&&c.pop(),o.cookie("gwHistory_n",c.join("_"),{path:"/",expires:7})}}),o.fn.headerSet({isIndex:"",hot:"1",search:"1",miniShop:"1",isAdbox:"1"})}(jQuery);